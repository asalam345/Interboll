                                element = element + '</div>';
                                element = element + '<div class="col-md-10" style="margin-right: 0px;padding: 10px;">';
                                element = element + '<div id="messageText" style="background:#4080FF; padding:5px 10px; border-radius:5px;color:#fff;text-align:left;float: right;">' + val.m_textMessage + '</div>';
