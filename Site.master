                            var sessionFromId = '<%= Session["CurrentUserId"].ToString() %>';
                            var getTime = val.createdDate;
                            var tim = getTime.split(" ");
                            var hhmm = tim[1].split(":");
                            if (fromId == sessionFromId) {
                                element = element + '<div class="col-md-12" style="padding: 0px 5px;">';
                                element = element + '<div id="messagePhotoDiv" class="col-md-2 pull-right" style="margin-right: -4px;padding: 5px;">';
