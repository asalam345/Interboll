    <script type="text/javascript">
        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }
        $(document).ready(function () {

            var tab = getCookie("tab");
            if (tab != "") {
                $(".menumin").removeClass('active');
                //$(".tab_pane_about").removeClass('active');

                $('#' + tab).addClass('active');
                $('#' + tab + "_1").addClass('active');
                if (tab == "follow") {
                    $("#follow_1_tab").addClass('active');
                    $('#11').addClass('active');
                    $("#follow_1").addClass('active');
                    $("#about_1").removeClass('active');
                    $("#photo_1").removeClass('active');
                    $("#outline_1").removeClass('active');
                }
                if (tab == "about") {
                    $("#follow_1").removeClass('active');
                    $("#photo_1").removeClass('active');
                    $("#outline_1").removeClass('active');
                    $("#about_1").addClass('active');
                    $("#Profileb").addClass('active');
                    $('#yourphoto2').addClass('active');
                    $('li.tabalbum').find('a[href*="#yourphoto2"]').addClass('active');
                }
                if (tab == "photo") {
                    $("#about_1").removeClass('active');
                    $("#follow_1").removeClass('active');
                    $("#photo_1").addClass('active');
                    $("#outline_1").removeClass('active');
                }
                if (tab == "outline") {
                    $("#about_1").removeClass('active');
                    $("#follow_1").removeClass('active');
                    $("#photo_1").removeClass('active');
                    $("#outline_1").addClass('active');
                }
            }
            $("#photo").click(function () { setCookie("tab", "photo", 5); });
            $("#follow").click(function () { setCookie("tab", "follow", 5); $("#follow_1_tab").addClass('active'); $("#follow_1").addClass('active'); $('#11').addClass('active'); });
            $("#about").click(function () { setCookie("tab", "about", 5); $('#Profileb').addClass('active'); });
            $("#outline").click(function () { setCookie("tab", "outline", 5); });
        });
        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }
    </script>
