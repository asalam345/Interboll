    <style>
         img.lazy-image {
            width: 100% ; 
            height: 150px;
            display: inline-block;
            background-image: url('../USERPANEL/img/loading/21.gif');
            background-repeat: no-repeat;
            background-position: 50% 50%;
        }

        img.lazy-image-small {
            width: 24px ; 
            height: 24px ;
            display: inline-block;
            background-image: url('../USERPANEL/img/loading/21.gif');
            background-repeat: no-repeat;
            background-position: 50% 50%;
        }
    </style>
    <style>
        .ui-widget-content {
