            });
        });

        $(function () {
            $('.lazy-image-small').lazy({
                placeholder: "data:image/gif;base64,R0lGODlhEALAPQAPzl5uLr9Nrl8e7..."
            });
        });

        function SetTarget() {
            document.forms[0].target = "_blank";
        }
